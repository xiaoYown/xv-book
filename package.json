{
	"name": "e-test",
	"displayName": "e-test",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:e-test.helloWorld",
		"onCommand:e-test.commond.updateTreeNode"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "e-test.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "e-test.commond.updateTreeNode",
				"title": "update tree node"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "treeView",
					"title": "tree view",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
      "treeView": [
        {
          "id": "online",
          "name": "目录"
        }
      ]
    }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
